{
  "version": 2,
  "name": "ukenbot",
  "builds": [
    { "src": "bot.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "bot.js" }
  ],
  "headers": [
    {
      "source": "Content-Security-Policy",
      "value": "default-src 'self'; script-src 'self' https://vercel.live"
    }
  ]
}
